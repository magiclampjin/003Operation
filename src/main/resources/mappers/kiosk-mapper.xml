<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="Kiosk">
	<select id="selectAll" resultType="com.operation.dto.KioskDTO">
		select * from kiosk where is_game = #{is_game};
	</select>
	
	<select id="selectById" resultType="com.operation.dto.KioskDTO">
		select * from kiosk where id = #{id};
	</select>
</mapper>